<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:id="@+id/latlng"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="TextView"
        android:textColor="@color/DarkerBlack"
        android:gravity="center"
        android:textSize="11sp"


        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/time"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        app:layout_constraintRight_toRightOf="@+id/time"
        android:layout_marginTop="1dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/time"
        app:layout_constraintTop_toTopOf="@+id/customname" />
    <TextView
        android:id="@+id/address"
        android:layout_width="186dp"
        android:layout_height="63dp"
        android:text="TextView"
        android:textColor="@color/DarkerBlack"
        android:gravity="center"
        android:textSize="11sp"

        android:layout_below="@+id/latlng"
        android:layout_alignEnd="@+id/latlng"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="54dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.0" />
    <TextView
        android:id="@+id/time"
        android:layout_width="150dp"
        android:layout_height="0dp"
        android:text="TextView"
        android:textColor="@color/DarkerBlack"
        android:gravity="center"
        android:textSize="11sp"

        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:layout_marginEnd="16dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/address"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/address" />
    <TextView
        android:id="@+id/customname"
        android:layout_width="188dp"
        android:layout_height="50dp"
        android:text="TextView"
        android:textColor="@color/DarkerBlack"
        android:gravity="center"
        android:textSize="11sp"


        android:layout_alignParentStart="true"
        android:layout_alignBottom="@+id/address"
        android:layout_below="@+id/latlng"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/address"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="5dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0" />


</android.support.constraint.ConstraintLayout>